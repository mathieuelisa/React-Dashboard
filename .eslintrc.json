{
  "env": {
    "browser": true,
    "es2021": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },

  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "react-hooks"],
  "rules": {
    "no-extra-boolean-cast": 0,
    "react-hooks/exhaustive-deps": "warn",
    "no-warning-comments": "warn",
    "no-console": "warn",
    "no-debugger": "warn",
    "no-unused-vars": "off"
  }
}
